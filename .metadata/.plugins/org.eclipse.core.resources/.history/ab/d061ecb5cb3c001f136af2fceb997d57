
public class ForDemo {

	public static void main(String[] args) {
		// 'A' --> 65 encoding
		// '65' --> A decoding
		int count =  0, line = 1;
		for(int i = 65 ; i <= 90 ; i++) {
			//System.out.printf("%c\t", (char)i);
			System.out.printf("%c\t", (char)(i + 32));			//소문자 출력
			else System.out.printf("%c\t", (char)(i));
			count++;
			if(count % 5 == 0); {
				System.out.println();
			}
		}
	}

}
